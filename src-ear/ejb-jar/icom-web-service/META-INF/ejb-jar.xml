<?xml version = '1.0' encoding = 'windows-1252'?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_3_0.xsd" version="3.0" xmlns="http://java.sun.com/xml/ns/j2ee">
  <enterprise-beans>   
     <session>
          <ejb-name>IcomSessionBean</ejb-name>
	  <ejb-class>icom.session.IcomSessionBean</ejb-class>
          <session-type>Stateless</session-type>
          <persistence-context-ref>
              <persistence-context-ref-name>icomEntityManager</persistence-context-ref-name>
              <persistence-unit-name>icomEntityManager</persistence-unit-name>
          </persistence-context-ref>
     </session> 
  </enterprise-beans>
</ejb-jar>
